<?xml version="1.0" encoding="utf-8"?>
<odoo>
<act_window
id="gmcmodule_membertable_action"
name="Member Data"
res_model="gmcmodule.membertable" />

<record id ="gmcmodule_membertable_view_form" model="ir.ui.view">
   <field name="name">Member Data Form</field>
   <field name="model">gmcmodule.membertable</field>
   <field name="arch" type="xml">
         <form>
 	     <group>
		<field name="name"/>
		<field name="age"/>
	     </group>
	 </form>
   </field>
</record>

<record id="gmcmodule_membertable_view_tree" model="ir.ui.view">
  <field name="name">Member Data List</field>
  <field name="model">gmcmodule.membertable</field>
  <field name="arch" type="xml">
    <tree>
	<field name="name"/>
	<field name="age"/>
    </tree>
    </field>
</record>		

</odoo>
